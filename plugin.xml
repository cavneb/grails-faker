<plugin name='faker' version='0.6' grailsVersion=' 2.0 &gt; *'>
  <author>Eric Berry</author>
  <authorEmail>cavneb@gmail.com</authorEmail>
  <title>Faker Plugin</title>
  <description>Faker, a port of Data::Faker from Perl, is used to easily generate fake data: names, addresses, phone numbers, etc.
</description>
  <documentation>http://github.com/cavneb/grails-faker</documentation>
  <type>FakerGrailsPlugin</type>
  <resources>
    <resource>com.coderberry.faker.FakerExampleController</resource>
    <resource>com.coderberry.faker.FakerService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>